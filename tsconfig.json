{
  "compilerOptions": {
    "module": "ES2015",
    "moduleResolution": "node",
    "target": "ES2017",
    "jsx": "react",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": [
      "node_modules/@types",
      "./types",
      "node_modules/axios",
      "node_modules/fetch"
    ]
  }
}